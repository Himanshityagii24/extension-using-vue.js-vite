{
    "manifest_version": 3,
    "name": "ZippiAi SidePanel",
    "version": "1.0.0",
    "background": {
        "service_worker": "src/background.js"
    },
    "permissions": [
        "sidePanel"  
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self';"
    },
    "side_panel": {
        "default_path": "index.html"
    }
}
